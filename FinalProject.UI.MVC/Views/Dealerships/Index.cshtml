        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZipCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car.Make)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DealershipId }) |
            @Html.ActionLink("Details", "Details", new { id=item.DealershipId }) |
